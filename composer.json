{
    "name": "patrickjde/contao-person-bundle",
    "type": "contao-module",
    "description": "Contao 4 person bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4"
    },
    "extra": {
        "contao-manager-plugin": "Patrickjde\\Contao\\PersonBundle\\ContaoManagerPlugin"
    },
    "autoload": {
        "psr-4": {
            "Patrickjde\\Contao\\PersonBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
